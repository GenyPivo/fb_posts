.container
  .col-md-5
    .panel.panel-default
      .panel-body
        - if @posts.present?
          - @posts.each do |post|
            = render partial: 'post', locals: { post: post }
        - else
          p
            | Posts are not found
  p
    = link_to 'Log out', destroy_user_session_path, method: :delete
